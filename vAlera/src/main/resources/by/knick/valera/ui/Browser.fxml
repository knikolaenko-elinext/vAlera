<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.web.WebView?>

<BorderPane prefHeight="800.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="by.knick.valera.ui.BrowserController">
   <center>
      <WebView fx:id="mainWebView" prefHeight="-1.0" prefWidth="-1.0" BorderPane.alignment="CENTER" />
   </center>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
   <top>
      <TextField fx:id="urlTextField" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets bottom="3.0" left="2.0" right="2.0"/>
         </BorderPane.margin>
      </TextField>
   </top>
</BorderPane>
